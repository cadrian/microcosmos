#! /usr/bin/env python

def fix_sys_path():
    import os
    import sys
    path=os.path.join(sys.path[0], "..", "src")
    sys.path.append(os.path.abspath(path))

def run():
    from net.cadrian.microcosmos.main import Game
    Game().run()


if __name__ == "__main__":
    fix_sys_path()
    run()
